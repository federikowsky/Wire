{
    "name": "wire",
    "description": "High-performance, zero-allocation HTTP parser for D - wrapping llhttp",
    "authors": [
        "Federico Filippi"
    ],
    "license": "MIT",
    "targetType": "library",
    "dependencies": {},
    "sourcePaths": [
        "source/wire",
        "source/wire/c"
    ],
    "dflags": [
        "-betterC=false"
    ],
    "lflags": [],
    "buildTypes": {
        "release": {
            "buildOptions": [
                "releaseMode",
                "optimize",
                "inline"
            ],
            "dflags": [
                "-O3",
                "-mcpu=native"
            ]
        },
        "debug": {
            "buildOptions": [
                "debugMode",
                "debugInfo"
            ]
        }
    }
}